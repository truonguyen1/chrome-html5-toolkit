{
  "name": "Toolkit Inspection",
  "description": "",
  "version": "0.1",
  "manifest_version": 2,
  "devtools_page": "devtool.html",
  "browser_action": {
    "default_icon": {
      "19": "icon48.png"
    },
    "default_title": "Toolkit inspection ON"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentscript.js"]
    }
  ],
  "background":{
      "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "webNavigation",
    "http://*/",
    "https://*/"
  ]
}